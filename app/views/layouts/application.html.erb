<!DOCTYPE html>
<html>
<head>
  <title>Ossguidepost</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if user_signed_in? %>
    <%= link_to_if user_signed_in?, "My Profile", user_path(current_user), :rel => "my_profile-show" %>
  <% else %>
    <%= link_to "Sign in with GitHub", user_omniauth_authorize_path(:github), :rel => "github-authentication" %>
  <% end %>


  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>

<%= yield %>

</body>
</html>
